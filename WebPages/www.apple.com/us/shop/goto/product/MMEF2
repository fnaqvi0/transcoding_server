        <script type="text/mustache" id="{{OverlayComponent}}">
<materializer data-uid='{{dataUid}}-mat'
                data-shown='{materializerShown}'
                data-show-done='{dataShowDone}'
                data-hide-done='{dataHideDone}'
                class='ase-overlay-materializer{{#if dataSheet}} ase-overlay-sheetmaterializer ase-overlay-materializersheet{{/if}}'>
    {{#if dataHasScreen}}
        <computed-height data-type="document" data-recompute="{recomputeScreen}"
                        class="ase-overlay-screen" data-property="min-height"></computed-height>
    {{/if}}
    <div class="ase-overlay {{#if dataSheet}}ase-overlay-sheet{{else}}ase-overlay-popup{{/if}}"
         {{#if dataAriaLabelledby}}aria-labelledby={{dataAriaLabelledby}}{{/if}}
                 role="dialog" tabindex="-1">
        <content />
    </div>
</materializer>

</script>

        <script type="text/mustache" id="{{Loader}}">
<div class="ase-loader
    {{^isLoaded}}ase-loader-isunloaded{{/isLoaded}}
    {{#isLoading}}ase-loader-isloading{{/isLoading}}
    {{#isLoaded}}ase-loader-isloaded{{/isLoaded}}">
    {{#if isLoaded}}
        {{{content}}}
    {{else}}
        <content />
    {{/if}}
</div>

</script>

        <script type="text/mustache" id="{{Validator}}">
<content/>
{{#if dataShowError}}
<materializer data-hide-height="0" data-show-height="computed"
              data-shown="dataErrorMsg">
    <div id="{{dataId}}-error"
         class="ase-validator-message form-message-wrapper {{dataErrorClass}}">
        {{#dataErrorMsg}}{{.}}{{/dataErrorMsg}}
    </div>
</materializer>
{{/if}}

</script>

        <script type="text/mustache" id="{{Placeholder}}">
{{#if dataPlaceholder}}
    <label {{#dataFor}}for="{{.}}"{{/dataFor}}>
        {{dataPlaceholder}}
    </label>
{{/if}}
<content />

</script>
    </body>
</html>
