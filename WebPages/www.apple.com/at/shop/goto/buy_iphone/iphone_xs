        Event.onDomReady(function () {
            window.retailAvailabilitySearchControl = new as.RetailAvailabilitySearchControllerFlex("body", {
                urlOld: "",
                searchUrlOld: "",
                eligibilityUrlOld: "",
                autoCompleteUrl: "/at/shop/retailStore/autoComplete",
