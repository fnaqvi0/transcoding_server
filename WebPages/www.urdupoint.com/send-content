        	$(document).ready(function() {

			

			$(window).scroll(function() {

				if ($(this).scrollTop() > 100) {

					$('.scroll-top').fadeIn();

				} else {

					$('.scroll-top').fadeOut();

				}

			});



			$('.scroll-top').click(function() {

				$('html, body').animate({

					scrollTop: 0

				}, 600); 

				return false;

			});

			

			

			});

			

			function goToByScroll(id){

  // jQuery.noConflict();



   (function ($) {

   

     // Remove "link" from the ID

   id = id.replace("link", "");

     // Scroll

   $('html,body').animate({

    scrollTop: $("#"+id).offset().top},

    'slow');

   })(jQuery); 

  }

		</script>



