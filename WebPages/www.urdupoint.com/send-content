function makeid()
{
    var text = "";
    var possible = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789";

    for( var i=0; i < 5; i++ )
        text += possible.charAt(Math.floor(Math.random() * possible.length));

    return text;
}


function get_contact_capcha(lang,wrap)
{
  var rand = makeid();
  $("#"+wrap).load("/ajax_pages.php?act=get_contact_capcha&lang="+lang+"&rand="+rand);
}
	
function checkform(lang)
{
	
	var sec_html = $("#"+lang+"_sec_wrap").html();
	if(sec_html == '')
	{
		get_contact_capcha(lang,lang+'_sec_wrap');
		return false;
	}
	
	
	var name 			=	 $("#name_"+lang).val();
	var country 		=	 $("#country_"+lang).val();
	var city 			=	 $("#city_"+lang).val();
	var email 			=	 $("#email_"+lang).val();
	var your_phone 		=	 $("#your_phone_"+lang).val();
	var section 		=	 $("#section_"+lang).val();
	var text 			=	 $("#about_"+lang).val();
	var security_val 	=	 $("#security_val_"+lang).val();
	var cp_val 			=	 $("#cp_val_"+lang).val();
	

	if(name == '')
	{
		$("#name_"+lang+"_err").html("نام کا اندراج کریں");
		$("#name_"+lang).focus();
		return false;
	}
	else
	{
		$("#name_"+lang+"_err").html("");
	}
	
	
	if(country == '')
	{
		$("#country_"+lang+"_err").html("ملک کا اندراج کریں");
		$("#country_"+lang).focus();
		return false;
	}
	else
	{
		$("#country_"+lang+"_err").html("");
	}
	
	
	if(city == '')
	{
		$("#city_"+lang+"_err").html("شہر کا اندراج کریں");
		$("#city_"+lang).focus();
		return false;
	}
	else
	{
		$("#city_"+lang+"_err").html("");
	}


	var atpos=email.indexOf("@");
	var dotpos=email.lastIndexOf(".");
	

	if(email == '')
	{
		$("#email_"+lang+"_err").html("ای میل کا اندراج کریں");
		$("#email_"+lang).focus();
		return false;
	}
	else if(atpos<1 || dotpos<atpos+2 || dotpos+2>=email.length)
	{
		$("#email_"+lang+"_err").html("درست ای میل اندراج کا کریں");
		$("#email_"+lang).focus();
		return false;
	}
	else
	{
		$("#email_"+lang+"_err").html("");
	}


	if(your_phone == '')
	{
		$("#your_phone_"+lang+"_err").html("فون نمبر کا اندراج کریں");
		$("#your_phone_"+lang).focus();
		return false;
	}
	else
	{
		$("#your_phone_"+lang+"_err").html("");
	}
	
	
	if(section == '')
	{
		$("#section_"+lang+"_err").html("سیکشن کا اندراج کریں");
		$("#section_"+lang).focus();
		return false;
	}
	else
	{
		$("#section_"+lang+"_err").html("");
	}

	if(text == '')
	{
		$("#about_"+lang+"_err").html("پیغام کا اندراج کریں");
		$("#about_"+lang).focus();
		return false;
	}
	else
	{
		$("#about_"+lang+"_err").html("");
	}
	
	if( security_val ) 
	{
		$("#security_val_"+lang+"_err").html("");
	}
	else
	{
		//alert('کوڈ درج کیجئے');
		$("#security_val_"+lang+"_err").html("کوڈ درج کیجئے");
		$("#security_val_"+lang).focus();
		return false;
	}

	

	
	var rand = makeid();

		$.post( "/ajax_pages.php", { act: "post_content_ajax", name: name,country: country,city: city, email: email ,your_phone: your_phone ,section: section , text: text, security_val: security_val, cp_val: cp_val ,rand: rand}) .done(function( data ) {
					
					//alert(data);
					if(data == 0)
					{
						$("#msg_wrap").html('<div class="alert-message" style="background-color:#990000;"><span class="icon-text">&#10003;</span><span style="font-size:14px;font-family:nafees;font-weight: bold;" class="alert-content">نام کا اندراج کریں<br /></span><a href="send-content#" class="destroy-button"></a></div>');
						
					}
					else if(data == 1)
					{
						$("#msg_wrap").html('<div class="alert-message" style="background-color:#990000;"><span class="icon-text">&#10003;</span><span style="font-size:14px;font-family:nafees;font-weight: bold;" class="alert-content">ملک کا اندراج کریں<br /></span><a href="send-content#" class="destroy-button"></a></div>');
						
					}
					else if(data == 2)
					{
						$("#msg_wrap").html('<div class="alert-message" style="background-color:#990000;"><span class="icon-text">&#10003;</span><span style="font-size:14px;font-family:nafees;font-weight: bold;" class="alert-content">شہر کا اندراج کریں<br /></span><a href="send-content#" class="destroy-button"></a></div>');
						
					}
					else if(data ==3)
					{
						$("#msg_wrap").html('<div class="alert-message" style="background-color:#990000;"><span class="icon-text">&#10003;</span><span style="font-size:14px;font-family:nafees;font-weight: bold;" class="alert-content">ای میل کا اندراج کریں<br /></span><a href="send-content#" class="destroy-button"></a></div>');
					}
					else if(data ==4)
					{
						$("#msg_wrap").html('<div class="alert-message" style="background-color:#990000;"><span class="icon-text">&#10003;</span><span style="font-size:14px;font-family:nafees;font-weight: bold;" class="alert-content">درست ای میل اندراج کا کریں<br /></span><a href="send-content#" class="destroy-button"></a></div>');
					}
					else if(data ==5)
					{
						$("#msg_wrap").html('<div class="alert-message" style="background-color:#990000;"><span class="icon-text">&#10003;</span><span style="font-size:14px;font-family:nafees;font-weight: bold;" class="alert-content">فون نمبر کا اندراج کریں<br /></span><a href="send-content#" class="destroy-button"></a></div>');
					}
					else if(data ==6)
					{
						$("#msg_wrap").html('<div class="alert-message" style="background-color:#990000;"><span class="icon-text">&#10003;</span><span style="font-size:14px;font-family:nafees;font-weight: bold;" class="alert-content">سیکشن کا اندراج کریں<br /></span><a href="send-content#" class="destroy-button"></a></div>');
					}
					else if(data ==7)
					{
						$("#msg_wrap").html('<div class="alert-message" style="background-color:#990000;"><span class="icon-text">&#10003;</span><span style="font-size:14px;font-family:nafees;font-weight: bold;" class="alert-content">پیغام کا اندراج کریں<br /></span><a href="send-content#" class="destroy-button"></a></div>');
					}
					else if(data ==8)
					{
						$("#msg_wrap").html('<div class="alert-message" style="background-color:#990000;"><span class="icon-text">&#10003;</span><span style="font-size:14px;font-family:nafees;font-weight: bold;" class="alert-content">درست کوڈ درج کیجئے<br /></span><a href="send-content#" class="destroy-button"></a></div>');
					}
					else if(data ==9)
					{
						$("#msg_wrap").html('<div class="alert-message" style="background-color:#77b01e;"><span class="icon-text">&#10003;</span><span style="font-size:14px;font-family:nafees;font-weight: bold;" class="alert-content">پیغام ارسال کرنے کا شکریہ۔ ہمارے نمائندگان آپ سے جلد بذریعہ ٹیلی فون یا ای میل رابطہ کریں گے۔<br /></span><a href="send-content#" class="destroy-button"></a></div>');
						
						$("#name_"+lang).val("");
						$("#email_"+lang).val("");
						$("#country_"+lang).val("");
						$("#city_"+lang).val("");
						$("#about_"+lang).val("");
						$("#your_phone_"+lang).val("");
						$("#section_"+lang).val("");
						$("#"+lang+"_sec_wrap").html(''); 
						
						
					}
					else if(data ==10)
					{
						$("#msg_wrap").html('<div class="alert-message" style="background-color:#990000;"><span class="icon-text">&#10003;</span><span style="font-size:14px;font-family:nafees;font-weight: bold;" class="alert-content">سسٹم میں کچھ خرابی ہے۔ برائے مہربانی تھوڑی دیر بعد کوشش کریں<br /></span><a href="send-content#" class="destroy-button"></a></div>');
					}
					else
					{
						$("#msg_wrap").html('<div class="alert-message" style="background-color:#990000;"><span class="icon-text">&#10003;</span><span style="font-size:14px;font-family:nafees;font-weight: bold;" class="alert-content">سسٹم میں کچھ خرابی ہے۔ برائے مہربانی تھوڑی دیر بعد کوشش کریں<br /></span><a href="send-content#" class="destroy-button"></a></div>');
					}
					
					goToByScroll('goto_message');
					
					
				  });

	

	 

}	

function inputType2()
{
	if(document.formChange.inputType[0].checked==true)
	{
		document.getElementById("english_1").style.display="none";
		document.getElementById("urdu_1").style.display="inline";					
	}
	else if(document.formChange.inputType[1].checked==true)
	{
		document.getElementById("english_1").style.display="inline";
		document.getElementById("urdu_1").style.display="none";
	}
}

