function show_this_txt(b)
{
	var protocol = window.location.protocol;
	var host_name = window.location.hostname;
	var pathname = window.location.pathname;
	var queryString = window.location.search;
	var curent_url = document.URL;
	var web = curent_url.search("mobile=w"); 
	var mob = curent_url.search("mobile=m"); 
	
	
	//var mobile_cookie = '';

	if($.cookie('mobile') === null || $.cookie('mobile') === "" || $.cookie('mobile') === "null" || $.cookie('mobile') === undefined)
	{
		  //no cookie
		var  mobile_cookie = '';
	}
	else
	{
		var mobile_cookie = $.cookie('mobile');
	}
	
	if(web > 0 )
	{
		$.cookie("mobile", "w");
		mobile_cookie = 'w';
	}
	
	if(mob > 0 )
	{
		$.cookie("mobile", "m");
		mobile_cookie = 'm';
	}
	
	
	//alert(mobile_cookie);
	
	
	if(mobile_cookie == '' || mobile_cookie == 'm' || mobile_cookie == 0  || mobile_cookie == null )
	{
		var redi_host = '';
		if(host_name == 'daily.urdupoint.com')
		{
			redi_host = 'm.urdupoint.com/daily';
		}
		else
		{
			redi_host = 'm.urdupoint.com';
		}
		//protocol+
		var redi_url = 'https://'+redi_host+pathname+queryString;
		//alert(redi_url);
		//$.cookie("mobile", "1");
		window.location.href = redi_url;
	}
	else
	{
		
	}
	
}

